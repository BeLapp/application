<body>
    <tu tuid="admin_access_listmodule">
      <tuv xml:lang="en">
        <seg>Edit user roles - access control</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Nutzerrollen editieren -Zugriffskontrolle</seg>
      </tuv>
    </tu>

    <tu tuid="admin_access_store">
      <tuv xml:lang="en">
        <seg>Edit user roles - access control</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Nutzerrollen editieren -Zugriffskontrolle</seg>
      </tuv>
    </tu>

    <tu tuid="admin_access_listrole">
      <tuv xml:lang="en">
        <seg>Access to Document</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Zugriff auf Dokument</seg>
      </tuv>
    </tu>

    <tu tuid="access_select_module">
      <tuv xml:lang="en">
        <seg>Overview of access control for role '%1$s'</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Übersicht der Zugriffskontrolle für Rolle '%1$s'</seg>
      </tuv>
    </tu>

    <tu tuid="access_current_role_name">
      <tuv xml:lang="en">
        <seg>
        You are editing access control for role '%1$s'.  Please select
        modules, resources and document transitions you would like to grant access to or enable.
        </seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>
        Sie bearbeiten gerade die Zugriffskontrolle für die Rolle '%1$s'.
        Bitte wählen Sie Module und Administrationsbereiche, für die Sie Zugriff gewähren möchten, und geben Sie an, 
		welche Zustandsänderungen des Dokuments diese Rolle durchführen darf.
        </seg>
      </tuv>
    </tu>

    <tu tuid="access_button_save">
      <tuv xml:lang="en">
        <seg>Save</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Speichern</seg>
      </tuv>
    </tu>

    <tu tuid="access_button_cancel">
      <tuv xml:lang="en">
        <seg>Cancel</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Abbrechen</seg>
      </tuv>
    </tu>

    <tu tuid="access_redirect_message">
      <tuv xml:lang="en">
        <seg>Done</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Fertig</seg>
      </tuv>
    </tu>

    <tu tuid="access_submit_save">
      <tuv xml:lang="en">
        <seg>Operation complete</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Vorgang abgeschlossen</seg>
      </tuv>
    </tu>

    <tu tuid="access_submit_cancel">
      <tuv xml:lang="en">
        <seg>Operation canceled</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Vorgang abgebrochen</seg>
      </tuv>
    </tu>

    <tu tuid="access_save_message">
      <tuv xml:lang="en">
        <seg>Your changes have been successfully saved</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Ihre Änderungen wurden gespeichert</seg>
      </tuv>
    </tu>

    <tu tuid="access_cancel_message">
      <tuv xml:lang="en">
        <seg>Your changes have been discarded</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Ihre Änderungen wurden verworfen</seg>
      </tuv>
    </tu>

    <tu tuid="access_select_role">
      <tuv xml:lang="en">
        <seg>Please select roles you would like to grant access to this document</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Bitte wählen Sie die Rollen, die Zugriff auf dieses Dokument erhalten sollen</seg>
      </tuv>
    </tu>

    <tu tuid="access_current_docid">
      <tuv xml:lang="en">
        <seg>Currently selected document ID:</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Momentan ausgewählte Dokumenten-ID:</seg>
      </tuv>
    </tu>

    <tu tuid="access_available_roles">
      <tuv xml:lang="en">
        <seg>Available roles</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Verfügbare Rollen</seg>
      </tuv>
    </tu>

    <tu tuid="admin_access_checked_in_guest">
      <tuv xml:lang="en">
        <seg>(Access already granted by 'guest' role)</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>(Bereits erlaubt durch 'guest'-Rolle)</seg>
      </tuv>
    </tu>

    <tu tuid="access_module_changes">
      <tuv xml:lang="en">
        <seg>Modules - Role has access to:</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Module - Rolle hat Zugriff auf:</seg>
      </tuv>
    </tu>
    
    <tu tuid="access_resource_changes">
      <tuv xml:lang="en">
        <seg>Sections of administration  - Role has access to:</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Bereiche der Administration - Rolle hat Zugriff auf:</seg>
      </tuv>
    </tu>
    
    <tu tuid="access_workflow_changes">
      <tuv xml:lang="en">
        <seg>Document status - Role can execute the following status changes:</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Dokumentstatus - Rolle kann folgende Zustandsänderungen durchführen:</seg>
      </tuv>
    </tu>

    <tu tuid="access_module_changes_affect_accounts">
      <tuv xml:lang="en">
        <seg>Changes will affect the following accounts</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Änderungen betreffen die folgenden Nutzerkonten</seg>
      </tuv>
    </tu>

</body>

