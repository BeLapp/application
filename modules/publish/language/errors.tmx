<!-- This language file contains all texts concerning field hints. -->

<!-- **************Errors for Person and their names****************** -->
    <tu tuid="publish_validation_error_person_invalid">
        <tuv xml:lang="en">
            <seg>Invalid datatype: please enter just a string.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Unerlaubter Datentyp: bitte tragen Sie ausschließlich eine Zeichenkette ein.</seg>
        </tuv>
    </tu>

    <tu tuid="publish_validation_error_person_notalpha">
        <tuv xml:lang="en">
            <seg>Error: %value% may only contain alphabetic characters.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Fehler: %value% darf nur alphabetische Zeichen enthalten.</seg>
        </tuv>
    </tu>

    <tu tuid="publish_validation_error_person_stringempty">
        <tuv xml:lang="en">
            <seg>This field is required.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Dieses Feld muss ausgefüllt werden.</seg>
        </tuv>
    </tu>

<!-- **************END Person************** -->

<!-- **************Errors for Date****************** -->
    <tu tuid="publish_validation_error_date_invalid">
        <tuv xml:lang="en">
            <seg>Invalid datatype: please enter just a string, an integer, array or Zend_Date.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Unerlaubter Datentyp: bitte tragen Sie ausschließlich eine Zeichenkette, eine Zahl, ein Array oder Zend_Date ein.</seg>
        </tuv>
    </tu>

    <tu tuid="publish_validation_error_date_invaliddate">
        <tuv xml:lang="en">
            <seg>Error: %value% does not appear to be a valid date.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Fehler: %value% scheint kein gültiges Datum zu sein.</seg>
        </tuv>
    </tu>

    <tu tuid="publish_validation_error_date_falseformat">
        <tuv xml:lang="en">
            <seg>Please change %value% to fit the date format %format%.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Bitte ändern Sie %value% in das Datumsformat %format%.</seg>
        </tuv>
    </tu>
<!-- **************END Date************** -->

    <tu tuid="publish_validation_error_year">
        <tuv xml:lang="en">
            <seg>Please enter a year that is greater that %min%.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Bitte tragen Sie ein Jahr ein, dass größer als %min% ist.</seg>
        </tuv>
    </tu>
<!-- **************END Year************** -->

    <tu tuid="publish_validation_error_notempty_isempty">
        <tuv xml:lang="en">
            <seg>This field is required.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Dieses Feld muss ausgefüllt werden.</seg>
        </tuv>
    </tu>
<!-- **************END NotEmpty************** -->
  
    <tu tuid="publish_validation_error_inarray_notinarray">
        <tuv xml:lang="en">
            <seg>The value %value% was not found in the selection.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Der Wert %value% wurde nicht in der Auswahl gefunden.</seg>
        </tuv>
    </tu>
<!-- **************END InArray************** -->

    <tu tuid="publish_validation_error_subjectmsc_msgsubjectmsc">
        <tuv xml:lang="en">
            <seg>The value %value% is not a valid MSC class.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Der Wert %value% ist keine gültige MSC Klasse.</seg>
        </tuv>
    </tu>
<!-- **************END SubjectMSC************** -->

    <tu tuid="publish_validation_error_int_notint">
        <tuv xml:lang="en">
            <seg>The value %value% is not a number.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Der Wert %value% ist keine Zahl.</seg>
        </tuv>
    </tu>
<!-- **************END Int************** -->
    
