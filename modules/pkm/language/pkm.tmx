<?xml version="1.0" encoding="UTF-8"?>
<tmx version="2.0" xmlns="http://www.lisa.org/tmx20">
  <header adminlang="en" creationtool="locale4j" creationtoolversion="1.1" o-tmf="unknown" segtype="block" srclang="*all*"/>
  <body>
    <tu tuid="pkm_modulename">
      <tuv xml:lang="en">
        <seg>Signature- and Key Management</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Signatur- und Schlüsselverwaltung</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_list_keys">
      <tuv xml:lang="en">
        <seg>Key Management</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Schlüsselverwaltung</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_key_nosign">
      <tuv xml:lang="en">
        <seg>Warning: This key cannot sign data!</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Achtung: Mit diesem Schlüssel können keine Daten signiert werden!</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_masterkey">
      <tuv xml:lang="en">
        <seg>Your internal key</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Interner Schlüssel</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_key_expired">
      <tuv xml:lang="en">
        <seg>This Key is expired!</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Dieser Schlüssel ist abgelaufen!</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_module_failure">
      <tuv xml:lang="en">
        <seg>GPG-Extension cannot get instantiated. You cannot use this module! Please check your parameters in config.ini.</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Die GPG-Erweiterung konnte nicht geladen werden! Daher kann dieses Modul nicht genutzt werden. Überprüfen Sie Ihre Parameter in der config.ini.</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_sigcheck_valid">
      <tuv xml:lang="en">
        <seg>Valid signature from %1$s</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Gültige Signatur von %1$s</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_sigcheck_invalid">
      <tuv xml:lang="en">
        <seg>Invalid signature from %1$s</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Ungültige Signatur von %1$s</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_verify_signatures">
      <tuv xml:lang="en">
        <seg>Signature verification</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Signaturüberprüfung</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_no_signature">
      <tuv xml:lang="en">
        <seg>No signature for this file</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Keine Signatur für diese Datei vorhanden</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_no_publication_selected">
      <tuv xml:lang="en">
        <seg>No (or invalid) publication has been selected for verification!</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Keine (oder eine ungültige) Publikation ausgewählt zur Signaturprüfung!</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_show_key">
      <tuv xml:lang="en">
        <seg>Show key</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Schlüssel anzeigen</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_sign_key">
      <tuv xml:lang="en">
        <seg>Sign key with your internal key</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Schlüssel mit internem Schlüssel signieren</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_delete_key">
      <tuv xml:lang="en">
        <seg>Remove key from keyring</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Schlüssel aus dem Schlüsselbund entfernen</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_disable_masterkey">
      <tuv xml:lang="en">
        <seg>Disable this key as internal key</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Schlüssel als internen Schlüssel deaktivieren</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_show_signature_key">
      <tuv xml:lang="en">
        <seg>Show key used for signature</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Signierschlüssel anzeigen</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_no_keys">
      <tuv xml:lang="en">
        <seg>No keys in your keyring</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Keine Schlüssel in Ihrem Schlüsselbund vorhanden</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_no_masterkey">
      <tuv xml:lang="en">
        <seg>No internal key found!</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Kein interner Schlüssel vorhanden!</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_key_noprivate">
      <tuv xml:lang="en">
        <seg>Your internal key does not have a private key!</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Der interne Schlüssel hat keinen Privatschlüssel!</seg>
      </tuv>
    </tu>
    <tu tuid="keyFileToUpload">
      <tuv xml:lang="en">
        <seg>Keyfile</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Schlüsseldatei</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_process">
      <tuv xml:lang="en">
        <seg>Send file</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Abschicken</seg>
      </tuv>
    </tu>
    <tu tuid="keyPassword">
      <tuv xml:lang="en">
        <seg>Passphrase for your internal key</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Passwort Ihres internen Schlüssels</seg>
      </tuv>
    </tu>
    <tu tuid="pkm_signFile">
      <tuv xml:lang="en">
        <seg>Sign this file</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Diese Datei signieren</seg>
      </tuv>
    </tu>
  </body>
</tmx>
