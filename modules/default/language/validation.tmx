  <body>

    <tu tuid="validation_error_date_invalid">
        <tuv xml:lang="en">
            <seg>Invalid datatype: please enter just a string, an integer, array or Zend_Date.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Unerlaubter Datentyp: bitte tragen Sie ausschließlich eine Zeichenkette, eine Zahl, ein Array oder Zend_Date ein.</seg>
        </tuv>
    </tu>

    <tu tuid="validation_error_date_invaliddate">
        <tuv xml:lang="en">
            <seg>Error: %value% does not appear to be a valid date.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Fehler: %value% scheint kein gültiges Datum zu sein.</seg>
        </tuv>
    </tu>

    <tu tuid="validation_error_date_falseformat">
        <tuv xml:lang="en">
            <seg>Please change %value% to fit the date format %format%.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Bitte ändern Sie %value% in das Datumsformat %format%.</seg>
        </tuv>
    </tu>
    
    <tu tuid="validation_error_year_invalid_format">
        <tuv xml:lang="en">
            <seg>Please provide a valid year value.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Bitte geben Sie eine gültige Jahreszahl an.</seg>
        </tuv>
    </tu>    
    
    <tu tuid="validation_error_year_invalid_negative">
        <tuv xml:lang="en">
            <seg>Please provide a positive year value.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Bitte geben Sie eine positive Jahreszahl an.</seg>
        </tuv>
    </tu>    
    
    <tu tuid="validation_error_year_too_large">
        <tuv xml:lang="en">
            <seg>Please provide a year less than 10000.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Bitte geben Sie eine Jahreszahl unter 10000 an.</seg>
        </tuv>
    </tu>

    <tu tuid="validation_error_int">
        <tuv xml:lang="en">
            <seg>Please provide a number.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Bitte geben Sie eine Zahl ein.</seg>
        </tuv>
    </tu>

    <tu tuid="validation_error_negative_number">
        <tuv xml:lang="en">
            <seg>Please provide a positive number (0...n).</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Bitte geben Sie eine positive Zahl ein (0..n).</seg>
        </tuv>
    </tu>

      <tu tuid="validation_error_unknown_enrichmentkey">
          <tuv xml:lang="en">
              <seg>Unknown enrichment key.</seg>
          </tuv>
          <tuv xml:lang="de">
